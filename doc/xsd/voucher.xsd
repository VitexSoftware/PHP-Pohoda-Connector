                    <xsd:documentation>Definice "Textové" nebo "Skladové" položky dokladu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="voucherLiquidationItem" type="vch:voucherliquidationItemType" minOccurs="0" maxOccurs="unbounded">
