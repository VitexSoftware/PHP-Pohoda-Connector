            <xsd:element name="liquidation" type ="vch:liquidationType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Likvidace.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
