                </xsd:annotation>
            </xsd:element>
            <xsd:element name="liquidationItem" type="vch:liquidationItemType" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Definice položek, které provádìjí likvidaci dokladu typu(Pohledávka/Závazek).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
