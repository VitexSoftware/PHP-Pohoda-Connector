                    <xsd:documentation>Text položky. Pokud nebude uveden, bude nastaven text z Globálního nastavení programu POHODA.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="quantity" type="xsd:float" fixed="1">
                <xsd:annotation>
                    <xsd:documentation>Množství na položce.Pro likvidaci dokladu vždy nastaveno "1".</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="payVAT" type="typ:boolean" fixed="false">
                <xsd:annotation>
                    <xsd:documentation>Jednotková cena s/bez DPH. Pri likvidaci dokladu vždy nastaveno "false" (bez DPH).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="rateVAT" type="typ:vatRateType">
