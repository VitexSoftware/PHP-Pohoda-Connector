            <xsd:element name="urlAddress" type="typ:contUrlAddressType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>URL adresa.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="shortcut" type="typ:contShortcutType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Zástupce.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>      
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="filesAttachmentsType">
        <xsd:sequence>
            <xsd:element name="companyDocumentsFolder" type="typ:string255" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Složka dokumentù firmy (pouze export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="subFolder" type="typ:string255" minOccurs="0">
