            </xsd:element>
            <xsd:element name="protocolVersion" minOccurs="0">
                <xsd:simpleType>
                    <xsd:annotation>
                        <xsd:documentation>Hlavní èíslo verze (tzv. major verze) protokolu pro komunikaci se servery finanèní správy. Výchozí hodnota je aktuální verze protokolu používaného v aplikaci POHODA.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:restriction base="xsd:integer">
                        <xsd:minInclusive value="1"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>

    <xsd:simpleType name="EETmodeType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="current">
                <xsd:annotation>
                    <xsd:documentation>Bìžný.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="simplified">
                <xsd:annotation>
                    <xsd:documentation>Zjednodušený.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="periodType">
        <xsd:annotation>
            <xsd:documentation>Úèetní období v úèetní jednotce programu POHODA -> (bìžné, pøechodné)</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="current"/>
            <xsd:enumeration value="following"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="EETItemType">
        <xsd:annotation>
            <xsd:documentation>Výèet možných typù položek z pohledu EET.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="BezTypu">
                <xsd:annotation>
                    <xsd:documentation>Bez typu.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="CestovniSluzba">
                <xsd:annotation>
                    <xsd:documentation>Cestovní služba.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Osvobozene">
                <xsd:annotation>
                    <xsd:documentation>Osvobozené plnìní.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="Poukaz">
                <xsd:annotation>
                    <xsd:documentation>Prodej nebo èerpání poukazu.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PouziteZakladni">
                <xsd:annotation>
                    <xsd:documentation>Použité zboží se základní sazbou.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PouziteSnizena1">
                <xsd:annotation>
                    <xsd:documentation>Použité zboží s první sníženou sazbou.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="PouziteSnizena2">
                <xsd:annotation>
                    <xsd:documentation>Použité zboží s druhou sníženou sazbou.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
  
    <xsd:simpleType name="PKPType">
        <xsd:restriction base="xsd:base64Binary">
            <xsd:length value="256"/>
        </xsd:restriction>
    </xsd:simpleType>
  
    <xsd:simpleType name="BKPType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{8}-[0-9a-fA-F]{8}-[0-9a-fA-F]{8}-[0-9a-fA-F]{8}">
            </xsd:pattern>
            <xsd:length value="44"/>
            <xsd:whiteSpace value="collapse"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="FIKType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-4[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}-[0-9a-fA-F]{2}">
            </xsd:pattern>
            <xsd:length value="39"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="dateTimeZoneType">
        <xsd:restriction base="xsd:dateTime">
            <xsd:pattern value="\d{4}-\d\d-\d\dT\d\d:\d\d:\d\d(Z|[+\-]\d\d:\d\d)"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="EETUUIDType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}">
            </xsd:pattern>
            <xsd:length value="36"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="CZDICType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="CZ[0-9]{8,10}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!--Záložka dokumenty-->
    <xsd:complexType name="attachmentsType">
        <xsd:annotation>
            <xsd:documentation>Záznamy ze záložky "Dokumenty".</xsd:documentation>
        </xsd:annotation>
        <xsd:choice maxOccurs="unbounded">
            <xsd:element name="files" type="typ:filesAttachmentsType" minOccurs="1" maxOccurs="1">
