                    <xsd:documentation>Podsložka, do níž budete ukládat dokumenty spojené s aktuálním záznamem.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="file" type="typ:contFileType" minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation>Soubor v adresáøi (pouze export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="contFileType">
        <xsd:all>
            <xsd:element name="name" type="typ:string255" minOccurs="0" />
            <xsd:element name="type" type="typ:string255" minOccurs="0" />
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="contUrlAddressType">
        <xsd:all>
            <xsd:element name="name" type="typ:string255" minOccurs="0" />      
            <xsd:element name="url" type="typ:string255" minOccurs="0" />
            <xsd:element name="dateOfChanged" type="xsd:dateTime" minOccurs="0" />
        </xsd:all>
    </xsd:complexType>

    <xsd:complexType name="contShortcutType">
        <xsd:all>      
            <xsd:element name="target" type="typ:string255" minOccurs="0" />
            <xsd:element name="agenda" type="typ:attachmentAgendaType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>V agendì Adresáø se rozlišuje typ: Adresáø/Události.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>

    <!-- **************************************************************************-->

</xsd:schema>
