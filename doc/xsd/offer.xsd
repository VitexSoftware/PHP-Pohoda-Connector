            </xsd:element>
        </xsd:all>
    </xsd:complexType>

    <xsd:simpleType name="offerTypeType">
        <xsd:annotation>
            <xsd:documentation>Typ nabídky.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="issuedOffer">
