            <xsd:element name="extId" type="typ:string64" minOccurs="0"/>
            <xsd:element name="parentCompStock" type="typ:parentCompStockType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>
                        Vazba položky na položkovou zásobu (komplet nebo souprava). Vazba se vytváøí pøes extId.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="text" type="typ:string90" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Text položky.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="quantity" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Množství.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="transferred" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Pøeneseno (pouze pro export).</xsd:documentation>
                </xsd:annotation>
