                <xsd:annotation>
                    <xsd:documentation>Druh služby (OSS).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PDP" type="xsd:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Pøíznak Pøenesení daòové povinnosti (pouze pro Vydané nabídky).</xsd:documentation>
                </xsd:annotation>
