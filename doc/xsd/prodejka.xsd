                    <xsd:documentation>Èinnost.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="contract" type="typ:refType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Zakázka.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="expirationDate" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Datum expirace.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EETItem" type="typ:EETItemType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Typ položky EET (pouze CZ verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="eKasaItem" type="pro:eKasaItemType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Položka eKasa (pouze SK verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="parameters" type="typ:parametersType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Volitelný parametr.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:all>
    </xsd:complexType>

    <xsd:simpleType name="prodejkaTypeType">
        <xsd:annotation>
            <xsd:documentation>Typ dokladu.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="saleVoucher">
                <xsd:annotation>
                    <xsd:documentation>Prodejka.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="deposit">
                <xsd:annotation>
                    <xsd:documentation>Vklad.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="withdrawal">
                <xsd:annotation>
                    <xsd:documentation>Výbìr.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
  
    <!-- eKasa -->
    <xsd:complexType name="eKasaType">
        <xsd:annotation>
            <xsd:documentation>Informace pro eKasu (pouze SK verze).</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="eKasaID" type="typ:string34" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Unikátny identifikátor dokladu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="eKasaOKP" type="typ:string44" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>OKP (Overovací kód podnikate¾a).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="eKasaParNumber" type="xsd:integer" minOccurs="0">
