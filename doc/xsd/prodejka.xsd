                    <xsd:documentation>Poradové èíslo paragónu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="eKasaDateOfPar" type="xsd:dateTime" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Dátum a èas vytvorenia paragónu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="printFM" type="typ:boolean" minOccurs="0" default="false">
                <xsd:annotation>
                    <xsd:documentation>Tlaè na FM.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="eKasaItemType">
        <xsd:annotation>
            <xsd:documentation>Informace pro eKasu na položce dokladu (pouze SK verze).</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="refId" type="typ:string44" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Unikátny identifikátor pôvodného dokladu - eKasa (používa sa pri vrátení tovaru).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="refundType" type="typ:eKasaRefundType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>eKasa typ vrátenia.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>  
        </xsd:sequence>
    </xsd:complexType>

