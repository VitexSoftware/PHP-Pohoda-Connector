                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <!-- Porovnání èástky -->
    <xsd:simpleType name="amountComparisonType">
        <xsd:annotation>
            <xsd:documentation>Porovnání èástky.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="equal">
                <xsd:annotation>
                    <xsd:documentation>Rovna.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="nonzero">
                <xsd:annotation>
                    <xsd:documentation>Rùzná od nuly.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
