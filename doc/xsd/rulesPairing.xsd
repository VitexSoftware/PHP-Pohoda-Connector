                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>
    <!-- Ukonèit -->
    <xsd:simpleType name="finishType">
        <xsd:annotation>
            <xsd:documentation>Ukonèit.</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="terminateMatching">
                <xsd:annotation>
                    <xsd:documentation>Ukonèit párování.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="terminateGroup">
                <xsd:annotation>
                    <xsd:documentation>Ukonèit skupinu - ádný doklad.</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- RESPONSE -->

    <xsd:element name="rulesPairingResponse" type="rul:rulesPairingResponseType">
        <xsd:annotation>
            <xsd:documentation>Odpovìï na import pravidla párování. Obsahuje podrobnosti zpracování a id importovaného záznamu.</xsd:documentation>
        </xsd:annotation>
    </xsd:element>


    <xsd:complexType name="rulesPairingResponseType">
        <xsd:complexContent>
            <xsd:extension  base="rdc:documentResponseType"/>
        </xsd:complexContent>
    </xsd:complexType>
</xsd:schema>