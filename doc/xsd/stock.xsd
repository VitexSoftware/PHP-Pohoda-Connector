            <xsd:element name="discount" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Oznaèení zásoby - Sleva.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>     
    
            <xsd:element name="prepare" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Oznaèení zásoby - Pøipravujeme.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>		
				
            <xsd:element name="availability" type="typ:string64" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Skladová dostupnost zásoby(skladem, do 3 dnù, na objednávku).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>	
		
