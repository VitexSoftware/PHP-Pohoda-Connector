                <xsd:annotation>
                    <xsd:documentation>Položky zásob u typù karta, služba, textová nebo výrobek.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="stockAttach" type="stk:stockAttachType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Svázané zásoby. Jako svázanou zásobu nelze vložit skladovou zásobu typu Komplet a Souprava (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="stockSerialNumber" type="stk:stockSerialNumberType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Výrobní èísla nebo šarže zásoby (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>  
            <xsd:element name="stockPriceItem" type="stk:stockPriceItemType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Slevy.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="attachments" type="typ:attachmentsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Záznamy ze záložky "Dokumenty".</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="print" type="prn:agendaPrintType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Po vytvoøení záznamu se provede jeho tisk.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="version" type="stk:stkVersionType" use="required"/>
    </xsd:complexType>

    <xsd:simpleType name="stkVersionType">
        <xsd:annotation>
            <xsd:documentation>Verze, které jsou dostupné k tomuto dokladu. Podrobnosti k jednotlivým verzím najdete na www.stormware.cz/xml</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="2.0"/>
            <xsd:enumeration value="2.1"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="actionTypeType">
        <xsd:choice>
            <xsd:element name="add" type="ftr:requestStockActionType">
                <xsd:annotation>
                    <xsd:documentation>Vložení nového záznamu. Výchozí hodnota.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="update" type="ftr:requestStockActionType">
                <xsd:annotation>
                    <xsd:documentation>Aktualizace dokladu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="delete" type="ftr:requestStockType">
                <xsd:annotation>
                    <xsd:documentation>odstranìní dokladu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="stockHeaderType">
        <xsd:all>
            <xsd:element name="id" type="typ:idType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>ID skladové zásoby (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="extId" type="typ:extIdType" minOccurs="0" >
                <xsd:annotation>
                    <xsd:documentation>
                        Odkaz na záznam v externí databázi. V databázi se nachází speciální tabulka obsahující
                        vazbu mezi agendou a externí databází.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <!-- Zásoba -->
            <xsd:element name="stockType" type="stk:stockTypeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Typ zásoby. Tento element je vyžadován pøi vytvoøení dokladu. Nelze mìnit pøi aktualizaci dokladu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="code" type="typ:stockIdsType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Øetìzcový identifikátor skladové položky.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EAN" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Èárový kód.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="20"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="PLU" type="xsd:integer" minOccurs="0" default="0">
                <xsd:annotation>
                    <xsd:documentation>PLU. Kód zásoby pro registraèní pokladnu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="isSales" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Odbyt. Urèuje, zda se má tato zásoba nabízet pøi vyskladnìní z agend Faktury, Prodejky a Výdejky.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="isSerialNumber" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Výrobní èísla. Urèuje, zda je toto èíslovaná zásoba, u které se mají evidovat výrobní èísla.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="isInternet" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Internet. Urèuje, zda je tato zásoba souèástí sortimentu nabízeného na internetu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="isBatch" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Šarže. Urèuje, zda je toto èíslovaná zásoba, u které se mají evidovat šarže.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="purchasingRateVAT" type="typ:vatRateType" minOccurs="0" default="none">
                <xsd:annotation>
                    <xsd:documentation>Sazba DPH pro nákup.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="sellingRateVAT" type="typ:vatRateType" minOccurs="0" default="none">
                <xsd:annotation>
                    <xsd:documentation>Sazba DPH pro prodej.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="name" type="typ:string90" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Název skladové položky. Tento element je vyžadován pøi vytvoøení dokladu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>		
            <xsd:element name="nameComplement" type="typ:string90" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Doplòující text èi poznámka k názvu skladové zásoby.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="unit" type="typ:unitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Mìrná jednotka.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="unit2" type="typ:unitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Mìrná jednotka 2.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="unit3" type="typ:unitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Mìrná jednotka 3.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="coefficient2" type="xsd:float" minOccurs="0" default="1.0">
                <xsd:annotation>
                    <xsd:documentation>Mìrná jednotka 2 koeficient.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="coefficient3" type="xsd:float" minOccurs="0" default="1.0">
                <xsd:annotation>
                    <xsd:documentation>Mìrná jednotka 3 koeficient.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="storage" type="typ:refTypeStorage" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Èlenìní. Tento element je vyžadován pøi vytvoøení dokladu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <!-- Ceny -->
            <xsd:element name="typePrice" type="typ:refType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Cenová skupina. Tento element je vyžadován pøi vytvoøení dokladu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="weightedPurchasePrice" type="typ:currencyType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Vážená nákupní cena (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="purchasingPrice" minOccurs="0" default="0">
                <xsd:annotation>
                    <xsd:documentation>Nákupní cena. Pokud není uvedena, bere se jako NULOVÁ.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:simpleContent>
                        <xsd:extension base="typ:currencyType">
                            <xsd:attribute name="payVAT" type="xsd:boolean" default="false">
                                <xsd:annotation>
                                    <xsd:documentation>Pøíznak urèuje zda se jedná o nákupní cenu s/bez DPH. Pokud není uveden atribut payVAT, jedná se o „Nákupní cena bez DPH“.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:attribute>
                        </xsd:extension>
                    </xsd:simpleContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="sellingPrice" minOccurs="0" default="0">
                <xsd:annotation>
                    <xsd:documentation>Prodejní cena. Pokud není uvedena, bere se jako NULOVÁ.</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:simpleContent>
                        <xsd:extension base="typ:currencyType">
                            <xsd:attribute name="payVAT" type="xsd:boolean" default="false">
                                <xsd:annotation>
                                    <xsd:documentation>Pøíznak urèuje zda se jedná o prodejní cenu s/bez DPH. Pokud není uveden atribut payVAT, jedná se o „Prodejní cena bez DPH“.</xsd:documentation>
                                </xsd:annotation>
                            </xsd:attribute>
                        </xsd:extension>
                    </xsd:simpleContent>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="fixation" type="stk:fixationType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Typ fixace (jen export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>		
            <xsd:element name="limitMin" type="xsd:float" minOccurs="0" default="0">
                <xsd:annotation>
                    <xsd:documentation>Limit Min pro automatickou objednávku.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="limitMax" type="xsd:float" minOccurs="0" default="0">
                <xsd:annotation>
                    <xsd:documentation>Limit Max pro automatickou objednávku.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="mass" type="xsd:float" minOccurs="0" default="0">
                <xsd:annotation>
                    <xsd:documentation>Hmotnost.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="volume" type="xsd:float" minOccurs="0" default="0">
                <xsd:annotation>
                    <xsd:documentation>Objem.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="count" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Stav zásoby (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="countReceivedOrders" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Objednávky (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="reservation" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Rezervace (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="supplier" type="typ:companyType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Dodavatel.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>				
            <xsd:element name="orderName" type="typ:string90" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Název skladové zásoby pro použití ve funkci Automatické objednávky.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="orderQuantity" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Požadované množství k objednání pro použití ve funkci Automatické objednávky.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>		
            <xsd:element name="countIssuedOrders" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Objednáno (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="reclamation" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Stav zásoby v reklamaci (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="service" type="xsd:float" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Servis (jen pro export).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
		
            <!-- záložka Doplòkové údaje -->
            <xsd:element name="shortName" type="typ:string24" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Zkrácený název skladové položky - napø. pøi tisku na registraèní pokladnì.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="typeRP" type="typ:refType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Pøíznak pro registraèní pokladnu.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="guaranteeType" type="typ:guaranteeTypeType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Typ záruky.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="guarantee" type="xsd:integer" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Délka záruky.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="producer" type="typ:string90" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Výrobce skladové položky.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="unitOfMeasure" type="typ:unitType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Jednotka mìrné ceny.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="coefficientOfMeasure" type="xsd:float" minOccurs="0" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Koeficient pro pøepoèet jednotky mìrné ceny.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EETItem" type="typ:EETItemType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Typ položky EET (pouze CZ verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="dicPP" type="typ:string18" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>DIÈ povìøujícího poplatníka (pouze CZ verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="eKasaItem" type="stk:eKasaItemType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Položky eKasa (pouze SK verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>   
            <xsd:element name="yield" type="typ:numberAccountType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Výnos. Úèet pro úètování výnosù z prodeje zásoby.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="cost" type="typ:numberAccountType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Náklad. Úèet pro úètování nákladu z prodeje zásoby.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="accountingPurchase" type="typ:accountingType" minOccurs="0" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Pøedkontace nákup. (Pouze pro DE/JÚ)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="accountingSale" type="typ:accountingType" minOccurs="0" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Pøedkontace prodej. (Pouze pro DE/JÚ)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classificationVATReceipt" type="typ:classificationVATType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Pøíjem - èlenìní DPH.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classificationKVDPHReceipt" type="typ:refType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Pøíjem - èlenìní KV DPH (pouze SK verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classificationVATIssue" type="typ:classificationVATType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Výdej - èlenìní DPH.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="classificationKVDPHIssue" type="typ:refType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Výdej - èlenìní KV DPH (pouze SK verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>    
            <xsd:element name="classOfStock" type="stk:classOfStockType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Úètování metodou A - Druh zásoby.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>	
            <xsd:element name="acc" type="typ:numberAccountType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Úètování metodou A - analytický úèet zásoby.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="typeServiceMOSS" type="typ:MOSStype" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Druh služby (OSS).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <!-- PDP -->
            <xsd:element name="controlLimitTaxLiability" type="typ:boolean" minOccurs="0" default="false">
                <xsd:annotation>
                    <xsd:documentation>Kontrola limitu pro režim pøenesení daòové povinnosti (pouze CZ verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="PDP" type="typ:boolean" minOccurs="0" default="false">
                <xsd:annotation>
                    <xsd:documentation>Pøíznak pøenesení daòové povinnosti (pouze SK verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PDPclassificationVATIssue" type="typ:classificationVATType" minOccurs="0" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Výdej  - èlenìní DPH související s režimem pøenesení daòové povinnosti (pouze SK verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>    
            <xsd:element name="PDPclassificationKVDPHIssue" type="typ:refType" minOccurs="0" nillable="true">
                <xsd:annotation>
                    <xsd:documentation>Výdej - èlenìní KVDPH související s režimem pøenesení daòové povinnosti (pouze SK verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CodePDP" type="typ:string8" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Kód zboží související s režimem pøenesení daòové povinnosti (pouze SK verze).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <xsd:element name="recyclingContrib" type="stk:recyclingContribType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Recyklaèní pøíspìvek zásoby.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>

            <!-- Intrastat -->
            <xsd:element name="intrastat" type="stk:instrastatType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Výkaz pro intrastat.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>	
			
            <!--Vlastnosti zásoby-->
            <xsd:element name="news" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Oznaèení zásoby - Novinka.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
		
            <xsd:element name="clearanceSale" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Oznaèení zásoby - Doprodej.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
		
            <xsd:element name="sale" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Oznaèení zásoby - Akce.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
		                                                
            <xsd:element name="recommended" type="typ:boolean" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Oznaèení zásoby - Doporuèujeme.</xsd:documentation>
